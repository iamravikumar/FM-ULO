@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title</h2>
<h3>@ViewBag.Message</h3>

<partial name="~/Views/Shared/_ApplicationDescription.cshtml" />

<h3>Debug Information</h3>
<dl>
    @{
        foreach (var kvp in Program.EnvironmentInfo)
        {
            <dt>@kvp.Key</dt>
            <dd>@kvp.Value</dd>
        }
    }
</dl>
